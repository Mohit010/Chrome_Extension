{
    "name" : "being qurious",
    "description" : "A chrome extension for efficient internet browsing",
    "version" : "1.0",
    "manifest_version" : 3,
    "icons" : {
        "16" : "images/icon-16.jpeg",
        "32" : "images/icon-32.jpeg",
        "48" : "images/icon-48.jpeg",
        "128": "images/icon-128.jpeg"
    },
    "content_scripts" : [
        {
            "js" : [ "scripts/popup.js" , "scripts/content.js"],
            "css" : ["scripts/content.css"],
            "matches" : [
                "*://*/*"   
            ]
        }
    ]
}